{
	"name": "DS_SV_Assistant",
	"author": "ZhenChen",
	"description": "DS小助手",
	"manifest_version": 3,
	"version": "1.3",
	"icons": {
		"16": "icons/tiger.png",
		"32": "icons/tiger.png",
		"48": "icons/tiger.png",
		"128": "icons/tiger.png"
	},
	"permissions": ["notifications", "storage", "tabs"],
	"action": {
		"default_popup": "popup/popup.html"
	},
	"content_scripts": [
		{
			// "matches": ["https://oa.desaysv.com/*"],
			"matches": ["<all_urls>"],
			"run_at": "document_idle",
			"js": [
				"js/jquery-3.5.1.min.js",
				"js/page1.js",
				"js/page2.js",
				"js/util.js",
				"js/storage.js",
				"js/content_script.js"
			]
		}
	],
	"background": {
		"service_worker": "js/background.js",
		"type": "module"
	}
}
